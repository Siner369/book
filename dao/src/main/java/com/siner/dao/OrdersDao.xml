            <result property="useableamount" column="useableamount"/>
            <result property="freezedamount" column="freezedamount"/>
            <result property="borrowlimit" column="borrowlimit"/>
            <result property="version" column="version"/>
            <result property="unreceiveinterest" column="unreceiveinterest"/>
            <result property="unreceiveprincipal" column="unreceiveprincipal"/>
            <result property="unreceiveamount" column="unreceiveamount"/>
            <result property="remainborrowlimit" column="remainborrowlimit"/>
        </association>-->

    </resultMap>

    <insert id="addOrder" parameterType="com.siner.entity.Orders">
        insert into orders(oid,uid,uaddress,utel,buytime,state) values(#{oid},#{uid},#{uaddress},#{utel},#{buytime},#{state})
	</insert>
    

</mapper>

